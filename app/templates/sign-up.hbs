<div class="row">
    <ol class="breadcrumb">
        <li>
            Sign Up
        </li>
    </ol>
</div>

{{input
    type='text'
    class='form-control'
    value=email
    placeholder='email'
}}
{{input
    type='password'
    class='form-control'
    value=password
    placeholder='password'
}}

<button  
    class='btn btn-default'
    {{action 'signUp'}}
> Sign Up
</button> 